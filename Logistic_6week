        "yp = - w1[1,0]/w1[2,0]*xp - w1[0,0]/w1[2,0]\n",
        "\n",
        "print('절편(w0) :', str(w1[0,0]))\n",
        "print('weight(w1) :', str(w1[1,0]))\n",
